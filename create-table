CREATE TABLE CRASH_EVENT {
    REPORT_NUMBER int PRIMARY KEY AUTO_INCREMENT,
    CRASH_DATE date,
    CRASH_TIME varchar(255),
    COUNTY varchar(255),
    CITY varchar(255),
    INVESTIGATING_AGENCY varchar(255),
    ON_STREET varchar(255),
    OFFSET_FEET int,
    OFFSET_DIRECTION varchar(255),
    FROM_INTERSECTING_STREET varchar(255),
    CRASH_SEVERITY varchar(255),
    LATITUDE float,
    LONGITUDE float
};

CREATE TABLE VEHICLE (
    REPORT_NUMBER int, 
    VEHICLE_NUMBER int, 
    YEAR int,
    MAKE varchar(255),
    MODEL varchar(255),
    COLOR varchar(255),
    TRAVELING_ON_STREET varchar(255),
    TRAVELING_DIRECTION varchar(255), 
    MANEUVER varchar(255),
    PRIMARY KEY (REPORT_NUMBER, VEHICLE_NUMBER),
    FOREIGN KEY (REPORT_NUMBER) REFERENCES CRASH_EVENT(REPORT_NUMBER)
);

CREATE TABLE DRIVER (
    REPORT_NUMBER int,
    VEHICLE_NUMBER int, 
    PERSON_NUMBER int,
    INJURY_SEVERITY varchar(255), 
    SEX char(1),
    AGE int, 
    RESTRAINT_SYSTEMS varchar(255),
    PRIMARY KEY (REPORT_NUMBER, VEHICLE_NUMBER, PERSON_NUMBER),
    FOREIGN KEY (REPORT_NUMBER, VEHICLE_NUMBER) REFERENCES VEHICLE(REPORT_NUMBER, VEHICLE_NUMBER)
);
